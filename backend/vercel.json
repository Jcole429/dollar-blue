{
  "version": 2,
  "builds": [
    {
      "src": "api/fetch_and_store.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/fetch_and_store",
      "dest": "api/fetch_and_store.py"
    },
    {
      "src": "/api/get_latest_blue",
      "dest": "api/get_latest_blue.py"
    },
    {
      "src": "/api",
      "dest": "api/index.py"
    },
    {
      "src": "/",
      "dest": "index.py"
    }
  ],
  "crons": [
    {
      "path": "/api/fetch_and_store",
      "schedule": "0 * * * *"
    }
  ]
}
